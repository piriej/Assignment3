<Window x:Class="Library.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        xmlns:views="clr-namespace:Library.Views"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:viewModels="clr-namespace:Library.ViewModels"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="MainWindow" Height="780" Width="470" Left="400" Top="50"
        Closing="Window_Closing"
        >

    <Grid Name="Panel">
        <ContentControl Grid.Row="1" prism:RegionManager.RegionName="{x:Static viewModels:RegionNames.ContentRegion}" />
            <!--<views:Borrowing/>
        </ContentControl>-->
    </Grid>
</Window>
