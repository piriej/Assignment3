<Window x:Class="Library.Hardware.Printer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library.Hardware"
        mc:Ignorable="d"
        Title="Printer" Height="400" Width="300" Left="900" Top="50"
        Closing="Window_Closing">
    <Grid Background="#FFE6E6E6">
        <Label x:Name="label" Content="Printer" HorizontalAlignment="Left" Margin="42,10,0,0" VerticalAlignment="Top" FontSize="32"/>
        <Label x:Name="errorMessageLabel" Content="Error Message" HorizontalAlignment="Left" Margin="10,368,0,0" VerticalAlignment="Top" Foreground="#FFF70606" FontSize="16" Width="274"/>
        <TextBox x:Name="printBox" HorizontalAlignment="Left" Height="363" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" Text="" VerticalAlignment="Top" Width="275" Margin="10,10,10,10"/>

    </Grid>
</Window>
